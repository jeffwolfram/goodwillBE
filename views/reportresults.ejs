<%- include('layout') %>
<%= console.log(data) %>
<body>
    <%- include('header') %>
    <h2>Date Range : <%= startDate %> - <%= endDate %></h2>
    <table border="1">
        <thead>
            <tr>
                <th>Date</th>
                <th>Employee Number</th>
                <th>Employee Name</th>
                <th>Total Good Count</th>
                <th>Total Bad Count</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(row) { %>
                <tr>
                    <td><%= row.record_date.toISOString().substring(0, 10) %></td>
                    <td><%= row.user_id %></td>
                    <td><%= row.user_name %></td>
                    <td><%= row.total_good %></td>
                    <td><%= row.total_bad %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>